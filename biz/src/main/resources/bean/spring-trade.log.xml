<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<!-- 订单日志信息 -->
	<bean id="tradeLogService" class="com.hsh24.dms.trade.service.impl.TradeLogServiceImpl">
		<property name="tradeLogDao" ref="tradeLogDao" />
	</bean>
	
	<bean id="tradeLogDao" class="com.hsh24.dms.trade.dao.impl.TradeLogDaoImpl" parent="baseDao" />
	
</beans>