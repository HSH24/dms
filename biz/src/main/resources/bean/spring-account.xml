<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<bean id="accountAction" class="com.hsh24.dms.account.action.AccountAction" parent="baseAction" scope="request">
		<property name="accountService" ref="accountService" />
	</bean>
	
	<!-- 账号管理 -->
	<bean id="accountService" class="com.hsh24.dms.account.service.impl.AccountServiceImpl">
		<property name="memcachedCacheService" ref="memcachedCacheService" />
	</bean>
	
</beans>