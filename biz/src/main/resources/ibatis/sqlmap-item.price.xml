<?xml version="1.0" encoding="GB2312"?>
<!DOCTYPE sqlMap
	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="item.price">
	
	<typeAlias alias="itemPrice" type="com.hsh24.dms.api.item.bo.ItemPrice" />
	
	<select id="getItemPrice" parameterClass="itemPrice" resultClass="itemPrice">
	<![CDATA[
		SELECT t.id,
			   t.price
		  FROM sup.sup_tb_item_price t
		 WHERE t.state= 'U'
		   AND t.item_region_id = #itemRegionId#
		   AND t.sku_id = #skuId#
	]]>
	</select>
	
</sqlMap>